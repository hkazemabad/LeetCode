class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        
        int size = nums.size();
       
        
       
        
        
        int i = 0;
        
        while (i < size)
        {
            if(nums[i] >= target)
                return i;
            else i++;
        }
        return i;
        
    }
};
